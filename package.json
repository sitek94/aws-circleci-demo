{
  "name": "aws-circleci-demo",
  "version": "1.0.0",
  "homepage": "https://sitek94.github.io/aws-circleci-demo",
  "repository": {
    "url": "https://github.com/sitek94/aws-circleci-demo"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:develop": "yarn build",
    "build:production": "yarn build",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint --ext .ts,.tsx src",
    "test": "vitest",
    "test:ci": "vitest --watch=false",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "serve": "serve build -p 3000 --single --no-clipboard",
    "cy:run": "cypress run --browser=chrome",
    "cy:open": "cypress open",
    "cy:ci:develop": "start-server-and-test 'yarn serve' http://localhost:3000 'yarn cy:run'"
  },
  "dependencies": {
    "@uiw/react-github-corners": "^1.5.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitest/ui": "latest",
    "c8": "^7.11.0",
    "cypress": "^9.5.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "jsdom": "latest",
    "prettier": "^2.5.1",
    "serve": "^13.0.2",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vite-plugin-eslint": "^1.3.0",
    "vitest": "^0.6.0"
  }
}
